<%- include('partials/header', { title: "Admin Dashboard" }) %>
<h1>Welcome, <%= username %> (Admin)</h1>
<h2>Registered Users:</h2>
<ul>
    <% users.forEach(user => { %>
        <li><%= user.username %> - <%= user.email %> - Role: <%= user.role %></li>
    <% }); %>
</ul>
<p><a href="/logout">Logout</a></p>
<%- include('partials/footer') %>
